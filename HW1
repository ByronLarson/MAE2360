Problem 6

#include <stdio.h>
#include <math.h>

double f(float x)
{
    return x-pow(x,3)/6+pow(x,5)/120;
}

int main ()
{
    float i;
    printf("x          f(x)    sin(x)  \n");
    printf("--------------------------\n");
    for(i=0;i<=2;i+=0.1)
    {
        printf("%f %f %f \n",i,f(i),sin(i));
        printf("--------------------------\n");
    }
    
    return 0;
}

