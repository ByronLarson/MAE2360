Problem 8

#include <stdio.h>
#include <math.h>

int main()
{
    float i,x,x_old;
    printf("Enter a guess: ");
    scanf("%f",&x);
    while(x<0 || x<1)
    {
        printf("Enter a value etween 0 and 1");
        scanf("%f",&x);
    }
    
    while(x_old!=x)
    {
        x_old=x;
        x=1/(1+(x*x));
        printf("Print vaules of %0.7f and %0.7f \n",x,x_old);
    }
    return 0;
}

